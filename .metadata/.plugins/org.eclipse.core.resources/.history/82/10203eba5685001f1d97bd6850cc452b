package Review;

public class ProductArray {

	public static void main(String[] args) {

		int[] num = { 10, 13, 333, 1234 };

		int product = 1;

		for (int i = 1; i < num.length; i++) {

			product *= i;
		}

		System.out.println(product);

//		for (int no : num) {
//
//			int result = product * no;
//
//			System.out.println(result);
//
//			product = result;
//
//		}

	}

}
